<p id="notice"><%= notice %></p>

<h1>Listing Velas</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Via</th>
      <th>Person</th>
      <th>Organization</th>
      <th>Fas</th>
      <th>Fse</th>
      <th>Code</th>
      <th>Instrument</th>
      <th>Description</th>
      <th>Diagnosis</th>
      <th>Resolved</th>
      <th>Closed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @velas.each do |vela| %>
      <tr>
        <td><%= vela.Date %></td>
        <td><%= vela.Via %></td>
        <td><%= vela.person %></td>
        <td><%= vela.Organization %></td>
        <td><%= vela.FAS %></td>
        <td><%= vela.FSE %></td>
        <td><%= vela.code %></td>
        <td><%= vela.Instrument %></td>
        <td><%= vela.Description %></td>
        <td><%= vela.Diagnosis %></td>
        <td><%= vela.resolved %></td>
        <td><%= vela.closed %></td>
        <td><%= link_to 'Show', vela %></td>
        <td><%= link_to 'Edit', edit_vela_path(vela) %></td>
        <td><%= link_to 'Destroy', vela, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vela', new_vela_path %>
